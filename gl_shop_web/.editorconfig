# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
insert_final_newline = true
indent_style = space

[*.py]
indent_size = 2

[{*.js, *.json}]
indent_size = 4

[*.html]
indent_size = 2

[{*.java}]
indent_size = 4

[{*.xml}]
indent_size = 4
